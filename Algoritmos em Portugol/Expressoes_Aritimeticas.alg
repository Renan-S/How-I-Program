algoritmo "Expressoes_Aritimeticas"

var
a,b,c,z,t:real

inicio
a <- 8+4
b <- 5-4
c <- 4.5*4 //Ponto no lugar da virgula
z <- 2^10
t <- a+b+c*z   //Expressão aritmética

escreval ("Valor de 'a':", a)
escreval ("Valor de 'b':", b)
escreval ("Valor de 'c':", c)
escreval ("Valor de 'z':", z)
escreval ("Valor de 't':", t)

fimalgoritmo
